{
  "name": "unplugin-autojs-hot-update",
  "version": "1.0.0",
  "description": "",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
